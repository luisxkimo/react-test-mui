{
	"ecmaFeatures": {
		"blockBindings": true,
		"forOf": true,
		"jsx": true, 
		"arrowFunctions": true,
		"classes": true,
		"modules": true
	},

    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },

	"rules": {
		"no-trailing-spaces": 0,
		"no-alert": 0,
		"no-unused-vars": 0,
		"coma-spacing": 0,
		"quotes": 0,
		"no-multi-spaces": 0,
	  	"key-spacing":0
	}
}